{
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {}
  },
  "env": {
      "browser": true,
      "node": true
  },
  "globals": {},
  "plugins": [],
  "extends": [
      "eslint:recommended"
  ],
  "rules": {
      "no-console": "off",
      "no-unreachable": "off",
      "semi": "off",
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "quote-props": ["error", "as-needed"]
  }
}
